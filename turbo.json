{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env"],
  "globalDotEnv": [".env"],
  "globalEnv": ["NODE_ENV"],
  "pipeline": {
    "build": {
      "outputs": [".next/**","!.next/cache/**"]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}